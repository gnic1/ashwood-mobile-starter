{
    "expo":  {
                 "name":  "Ashwood and Co",
                 "slug":  "ashwood-mobile",
                 "version":  "0.1.0",
                 "orientation":  "portrait",
                 "icon":  "./assets/icon.png",
                 "splash":  {
                                "image":  "./assets/splash.png",
                                "resizeMode":  "contain",
                                "backgroundColor":  "#0b0b0d"
                            },
                 "assetBundlePatterns":  [
                                             "**/*"
                                         ],
                 "ios":  {
                             "supportsTablet":  true
                         },
                 "android":  {
                                 "usesCleartextTraffic":  true,
                                 "adaptiveIcon":  {
                                                      "foregroundImage":  "./assets/adaptive-icon.png",
                                                      "backgroundColor":  "#0b0b0d"
                                                  },
                                 "package":  "com.ashwoodco.ashwoodmobile"
                             },
                 "web":  {
                             "bundler":  "metro",
                             "favicon":  "./assets/favicon.png"
                         },
                 "plugins":  [
                                 "expo-asset"
                             ]
             }
}
